#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int t;cin>>t;
	while(t--){
	    int n; cin>>n;
	    int o=0,z=0;
	    string s;
	    cin>>s;
	    
	    for(int i=0;i<n;i++){
	   if(s[i]=='1'){
	        o++;
	       
	   }else{
	        z++;
	   }
	   
	    }
	    
	    if(o%2 !=0 && z%2 !=0){
	        cout<<"no"<<endl;
	    }
	    else{
	        cout<<"yes"<<endl;
	    }
	  }
	return 0;
}
